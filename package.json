{
  "name": "@javascript-packages/roman-numerals",
  "version": "0.0.0-development",
  "description": "Easily convert integers to roman numerals in one line of code.",
  "license": "MIT",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "coverage": "tsdx test --coverage",
    "lint": "tsdx lint",
    "format": "prettier --write ./**/*.ts",
    "commit": "git-cz && git push",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "semantic-release": "semantic-release --branches main"
  },
  "dependencies": {},
  "devDependencies": {
    "@size-limit/preset-small-lib": "^7.0.5",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^7.0.4",
    "semantic-release": "^18.0.1",
    "size-limit": "^7.0.5",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.5.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/javascript-packages/roman-numerals.git"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "author": "Stefan KÃ¼hnel",
  "module": "dist/roman-numerals.esm.js",
  "size-limit": [
    {
      "path": "dist/roman-numerals.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/roman-numerals.esm.js",
      "limit": "10 KB"
    }
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
